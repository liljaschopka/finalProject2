<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<StackPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hi.userinterface.finalproject2.FacultyLoginController">
    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="320.0">
        <children>
            <Label alignment="CENTER_RIGHT" layoutX="70.0" layoutY="107.0" prefHeight="18.0" prefWidth="79.0" text="Name:">
                <font>
                    <Font size="16.0" />
                </font>
            </Label>
            <TextField fx:id="fxName" layoutX="164.0" layoutY="100.0" prefHeight="34.0" prefWidth="188.0" />
            <Label alignment="CENTER" layoutX="154.0" layoutY="14.0" text="Signing in as a Faculty Member:">
                <font>
                    <Font size="18.0" />
                </font>
            </Label>
            <TextField fx:id="fxDepartment" layoutX="164.0" layoutY="140.0" prefHeight="34.0" prefWidth="188.0" />
            <Label alignment="CENTER_RIGHT" layoutX="70.0" layoutY="147.0" text="Department:">
                <font>
                    <Font size="16.0" />
                </font>
            </Label>
        </children>
    </AnchorPane>
    <HBox alignment="CENTER" spacing="20.0">
        <children>
            <Button mnemonicParsing="false" onMouseClicked="#handleOKClicked" text="OK" />
            <Button mnemonicParsing="false" onMouseClicked="#handleCancelClicked" text="Cancel" />
        </children></HBox>
</StackPane>
